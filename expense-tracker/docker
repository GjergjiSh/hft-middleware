docker network create expense-sql-net
docker network ls
docker container run --name mysqldb --network expense-sql-net -e MYSQL_ROOT_PASSWORD=root -e MYSQL_DATABASE=expensedb -d mysql:8
docker container logs -f mysqldb
docker container exec -it mysqldb bash
mysql -uroot -proot
show databases;
docker image build -t expense-tracker .
docker container run --network expense-sql-net --name expense-tracker-container -p 8080:8080 -d expense-tracker
docker container logs -f expense-tracker-container

docker-compose down -v
docker-compose up -d